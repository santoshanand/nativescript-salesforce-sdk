{
  "compilerOptions": {
    "declaration": true,
    "outDir": "lib",
    "target": "es5",
    "module": "commonjs",
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "./node_modules/tns-core-modules/*",
        "./node_modules/*"
      ]
    },
    "sourceMap": true,
    "pretty": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noEmitHelpers": true,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules"
    ],
    "types": [
    ],
    "lib": [
        "es6",
        "dom"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "sample",
    "node_modules"
  ]
}